{
  "name": "@cdp/i18n",
  "description": "CDP i18n utility module",
  "version": "2.0.0",
  "main": "dist/cdp.i18n.js",
  "types": "dist/@types/cdp.i18n.d.ts",
  "author": {
    "name": "Shin Ogata",
    "email": "Shin.Ogata@jp.sony.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CDP-Tokyo/cdp-js.git"
  },
  "bugs": {
    "url": "https://github.com/CDP-Tokyo/cdp-js/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "CDP",
    "TypeScript",
    "jquery"
  ],
  "scripts": {
    "setup:tasks": "node ../master-tasks/provider.js",
    "postinstall": "npm run rearrange",
    "test": "run-s -l clean build lint unit-test",
    "ci": "run-s -l clean build lint unit-test metrics document",
    "package": "run-s ci && node ./tasks/clean.js --built --test --temp",
    "update": "npm update --save-dev && npm update --save && npm run package",
    "depends": "npm list --depth=1",
    "depends:prod": "npm list --depth=0 --only=prod",
    "depends:dev": "npm list --depth=0 --only=dev",
    "debug": "testem -f ./tests/runner/testem-amd.js",
    "clean": "node ./tasks/clean.js",
    "clean:temp": "node ./tasks/clean.js --temp",
    "clean:coverage": "node ./tasks/clean.js --coverage",
    "clean:typedoc": "node ./tasks/clean.js --typedoc",
    "clean:test": "node ./tasks/clean.js --test",
    "clean:built": "node ./tasks/clean.js --built",
    "clean:pkg": "node ./tasks/clean.js --pkg",
    "build": "run-s compile:ts:src bundle minify compile:ts:module",
    "compile:ts:src": "tsc -p ./src/tsconfig.all.json && tsc -p ./src/tsconfig.entry.json",
    "compile:ts:module": "tsc -p ./tsconfig.module.json",
    "compile:ts:metrics": "tsc -p ./tsconfig.json",
    "bundle": "run-s bundle:*",
    "bundle:embed": "node ./tasks/bundle.js --no-banner",
    "bundle:external": "node ./tasks/bundle-adjuster.js && webpack --config ./webpack.config.js && node ./tasks/bundle-finalizer.js",
    "minify": "uglifyjs dist/cdp.i18n.js -o dist/cdp.i18n.min.js --comments /^!/ --source-map includeSources,url=cdp.i18n.min.js.map",
    "lint": "run-p -l lint:*",
    "lint:js": "eslint tasks/**/*.js src/**/*.js -c tests/eslint/eslintrc.json",
    "lint:ts": "tslint --type-check -p ./tsconfig.json -c ./tests/tslint/tslint.json -e src/**/*.d.ts",
    "unit-test": "npm run coverage:exec",
    "unit-test:ci": "testem ci -f ./tests/runner/testem-ci.js",
    "coverage": "run-s clean:coverage build coverage:exec",
    "coverage:exec": "run-s coverage:instrument unit-test:ci coverage:remap coverage:report",
    "coverage:instrument": "nyc instrument ./built ./built --source-map=false",
    "coverage:remap": "node ./tasks/remap-coverage.js",
    "coverage:report": "nyc report --reporter=lcov --reporter=html --reporter=text --report-dir=docs/reports/coverage --temp-directory=docs/reports/coverage",
    "rearrange": "node ./tasks/external-rearrange.js --no-map",
    "document": "typedoc --out docs/typedoc/ src/scripts/tsconfig.json",
    "metrics": "npm run compile:ts:metrics && plato -r -d docs/reports/metrics -e ./tests/eslint/eslintrc.json built/CDP/**/*.js"
  },
  "dependencies": {
    "jquery": ">=1.8.3"
  },
  "devDependencies": {
    "@types/i18next": "^2.3.38",
    "@types/jasmine": "^2.5.52",
    "@types/jquery": "^3.2.3",
    "@types/requirejs": "^2.1.29",
    "convert-source-map": "^1.5.0",
    "del": "^3.0.0",
    "eslint": "^4.0.0",
    "fs-extra": "^3.0.1",
    "i18next": "^8.4.2",
    "i18next-browser-languagedetector": "^1.0.1",
    "i18next-localstorage-cache": "^1.1.1",
    "i18next-sprintf-postprocessor": "^0.2.2",
    "i18next-xhr-backend": "^1.4.2",
    "jquery-i18next": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.0.2",
    "plato": "^1.7.0",
    "requirejs": "^2.3.3",
    "source-map": "^0.5.6",
    "source-map-loader": "^0.2.1",
    "tslint": "^5.4.3",
    "typedoc": "^0.7.1",
    "typescript": "^2.4.0",
    "uglify-js": "^3.0.19",
    "webpack": "^3.0.0"
  }
}
