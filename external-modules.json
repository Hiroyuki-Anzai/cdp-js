{
    "root": "external",
    "scripts": "scripts",
    "stylesheets": "stylesheets",
    "modules": {
        "jquery": {
            "vender": "jquery",
            "scripts": {
                "ignore": [
                    "**/*.slim.*",
                    "**/*.map"
                ]
            }
        },
        "backbone": {
            "scripts": {
                "src": "*.js"
            }
        },
        "requirejs": {
            "scripts": {
                "cwd": ".",
                "src": "*.js"
            }
        },
        "underscore": {
            "scripts": {
                "src": "*.js"
            }
        },
        "i18next": {
            "scripts": {
                "cwd": "./dist/umd",
                "src": "*.js"
            }
        },
        "i18next-browser-languagedetector": {
            "vender": "i18next",
            "scripts": {
                "cwd": "./dist/umd",
                "src": "*.js"
            }
        },
        "i18next-localstorage-cache": {
            "vender": "i18next",
            "scripts": {
                "cwd": "./dist/umd",
                "src": "*.js"
            }
        },
        "i18next-sprintf-postprocessor": {
            "vender": "i18next",
            "scripts": {
                "cwd": "./dist/umd",
                "src": "*.js"
            }
        },
        "i18next-xhr-backend": {
            "vender": "i18next",
            "scripts": {
                "cwd": "./dist/umd",
                "src": "*.js"
            }
        },
        "jquery-i18next": {
            "vender": "i18next",
            "scripts": {
                "cwd": "./dist/umd",
                "src": "*.js"
            }
        },
        "flipsnap": {
            "scripts": {
                "cwd": ".",
                "src": "*.js"
            }
        },
        "cdp-ui-jqm": {
            "vender": "cdp",
            "component": { }
        }
    }
}
