{
    "root": "external",
    "scripts": "scripts",
    "stylesheets": "stylesheets",
    "modules": {
        "jquery": {
            "vender": "jquery",
            "scripts": {
                "ignore": [
                    "**/core.*",
                    "**/*.slim.*",
                    "**/*.map"
                ]
            }
        },
        "backbone": {
            "scripts": {
                "src": "*.js"
            }
        },
        "underscore": {
            "scripts": {
                "src": "*.js"
            }
        },
        "flipsnap": {
            "scripts": {
                "cwd": ".",
                "src": "*.js"
            }
        },
        "cdp-ui-jqm": {
            "vender": "cdp",
            "component": {}
        }
    }
}
