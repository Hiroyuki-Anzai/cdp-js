{
  "name": "@cdp/mobile",
  "description": "CDP SDK module for mobile",
  "version": "2.0.0",
  "private": true,
  "main": "dist/cdp.js",
  "types": "dist/@types/index.d.ts",
  "author": {
    "name": "Shin Ogata",
    "email": "Shin.Ogata@jp.sony.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CDP-Tokyo/cdp-js.git"
  },
  "bugs": {
    "url": "https://github.com/CDP-Tokyo/cdp-js/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "CDP",
    "TypeScript",
    "jquery"
  ],
  "scripts": {
    "setup": "npm install && node ./tasks/setup.js",
    "update": "npm update --save-dev && npm update --save && node ./tasks/update.js",
    "update:dist": "node ./tasks/update.js --no-build",
    "ci": "npm install && node ./tasks/setup.js && npm test && node ./tasks/update.js --command=ci",
    "test": "eslint tasks/**/*.js -c eslintrc.json"
  },
  "projectConfig": {
    "distributionTarget": "cdp-mobile",
    "modules": [
      "cdp-core",
      "cdp-lazyload",
      "cdp-promise",
      "cdp-i18n",
      "cdp-framework-jqm",
      "cdp-tools",
      "cdp-ui-listview",
      "cdp-ui-jqm",
      "cdp-nativebridge",
      "cdp-mobile"
    ]
  },
  "dependencies": {
    "@types/backbone": "^1.3.34",
    "@types/i18next": "^8.4.0",
    "@types/jquery": "^3.2.5",
    "@types/underscore": "^1.8.1",
    "backbone": ">=1.3.3",
    "jquery": ">=1.8.3",
    "underscore": ">=1.8.3"
  },
  "devDependencies": {
    "del": "^3.0.0",
    "eslint": "^4.1.1",
    "fs-extra": "^3.0.1"
  }
}